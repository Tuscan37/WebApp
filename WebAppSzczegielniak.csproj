      <_ContentIncludedByDefault Remove="WebAppTestProject\obj\Debug\net8.0\WebAppTestProject.sourcelink.json" />
      <_ContentIncludedByDefault Remove="WebAppTestProject\obj\project.assets.json" />
      <_ContentIncludedByDefault Remove="WebAppTestProject\obj\project.packagespec.json" />
