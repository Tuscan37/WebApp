        </PackageReference>
        <PackageReference Include="Microsoft.EntityFrameworkCore.Sqlite" Version="6.0.27"/>
        <PackageReference Include="Microsoft.EntityFrameworkCore.Tools" Version="6.0.27"/>
        <PackageReference Include="Npgsql.EntityFrameworkCore.PostgreSQL" Version="9.0.0-preview.2" />
    </ItemGroup>

    <ItemGroup>
      <Compile Remove="WebAppTestProject\**" />
    </ItemGroup>

    <ItemGroup>
      <EmbeddedResource Remove="WebAppTestProject\**" />
    </ItemGroup>

    <ItemGroup>
      <Content Remove="WebAppTestProject\**" />
    </ItemGroup>

    <ItemGroup>
      <_ContentIncludedByDefault Remove="WebAppTestProject\bin\Debug\net8.0\WebAppTestProject.deps.json" />
